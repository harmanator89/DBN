<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DBN.Views.Menu.SaveMe"
             Title="Save Me!">
    <ContentPage.Content>
       
        <StackLayout>
            <Label Text="Can't find your destination?" FontSize="Large" />
            <Label Text="Click the 'Save me!' button" />
            <Button Text="Save me!" Command="{Binding NavigationSMHelp}" Clicked="Button_Clicked"/>
            

        </StackLayout>

    </ContentPage.Content>
</ContentPage>